{
    "presets": [
        [
        "es2015",
        {
            "modules": false
        }
        ],
        "react",
        "stage-0"
    ],
    "plugins": [
        "transform-runtime"
    ],
    "env": {
        "production": {
            "presets": [
                "react-optimize"
            ]
        },
        "development": {
            "plugins": [
                "react-hot-loader/babel"
            ]
        }
    }
}