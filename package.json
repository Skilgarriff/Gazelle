{
  "name": "go-starter-kit",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "build": "BABEL_ENV=production NODE_ENV=production webpack --progress --colors --bail",
    "lint:js": "eslint client",
    "lint:css": "stylelint \"client/**/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "olebedev",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.2",
    "babel-core": "^6.5.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "6.5.0",
    "css-loader": "^0.23.1",
    "cssrecipes-defaults": "^0.5.0",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-react": "^3.16.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.8.5",
    "history": "2.0.0",
    "normalize.css": "^3.0.3",
    "postcss-functions": "^2.1.0",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "proxy-middleware": "^0.15.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^3.0.0",
    "react-hot-loader": "next",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "style-loader": "^0.13.0",
    "stylelint": "^7.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1",
    "whatwg-fetch": "^0.11.0"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        2
      ],
      "quotes": [
        2,
        "single"
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "react/jsx-uses-react": 2,
      "react/jsx-uses-vars": 2,
      "react/react-in-jsx-scope": 2
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ]
  }
}
